<!DOCTYPE html>
<head>
    <meta charset="utf-8">
    <title>首页</title>
    <link href="../../stylesheets/differ.css" rel="stylesheet">
    <script src="../../javascripts/html5.js"></script>
</head>
<body>
<section id="newest">
    <ul>
        <li class="tr-one">newest list</li>
        <li><input type="button" id="newAdd" value="添加"></li>
    </ul>
    <table>
        <tr>
            <td class="td-one">编号</td><td class="td-title">标题</td><td>类型</td><td>联系人</td>
            <td>学校</td><td><a class="newEdit" href="#">编辑</a><a class="newremove" href="#">删除</a></td>
        </tr>
    </table>
    <table class="Mcycle">
        <%docs.forEach(function(docs,index){%>
        <tr>
            <td class="td-one"><%=index%></td>
            <td class="td-title"><%=docs.title%></td>
            <td><%=docs.type%></td>
            <td><%=docs.linkman%></td>
            <td><%=docs.school%></td>
            <td><a href="#" class="newedit">编辑/</a><a href="#" class="newremove">删除</a></td>
        </tr>
        <% })%>
    </table>


    <div class="tab-form" id="newEditForm">
        <span>最新</span>
        <form>
            <ul>
                <li><label for="title">标题</label><input id="title" name="title" type="text"></li>
                <li><label for="type">类型</label>
                    <select id="type" name="type">
                        <option value="手机电脑">手机电脑</option><option value="其它">其它</option>
                    </select></li>
                <li><label for="linkman">联系人</label><input id="linkman" name="linkman" type="text"></li>
                <li><label for="school">联系人</label><input id="school" name="school" type="text"></li>
                <li><input type="button" class="tab-submit" value="提交"><input class="tab-cancel" type="button" value="取消"></li>
            </ul>
        </form>
        <div></div>
    </div>
</section>
<script src="../../javascripts/jquery.js"></script>
<script src="../../javascripts/jquery-dragMove.js"></script>
<script src="../javascripts/user.js"></script>
</body>
</html>

















































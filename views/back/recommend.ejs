<!DOCTYPE html>
<head>
    <meta charset="utf-8">
    <title>首页</title>
    <link href="../../stylesheets/differ.css" rel="stylesheet">
    <script src="../../javascripts/html5.js"></script>
</head>
<body>
<section id="recommendList">
    <table>
        <tr>
            <td class="td-one" id="two">编号</td><td class="td-title">标题</td><td>类型</td><td>联系人</td>
            <td>学校</td><td><a  href="#">编辑</a><a  href="#">删除</a></td>
        </tr>
    </table>
    <table class="Mcycle">
        <%docs.forEach(function(docs,index){%>
        <tr>
            <td class="td-one" data-id="<%=docs._id%>"><%=index%></td>
            <td class="td-title"><%=docs.title%></td>
            <td><%=docs.type%></td>
            <td><%=docs.linkman%></td>
            <td><%=docs.school%></td>
            <td class="tdEdit"><a href="#" id="recommendEdit">编辑/</a><a href="#" id="recommendRemove">删除</a></td>
        </tr>
        <% })%>
    </table>
    <div class="tab-form" id="popEditForm">
        <span>用户表</span>
        <form>
            <ul>
                <li>
                    <label>类型</label>
                        <select name="popType" id="one">
                            <option value="无">--选择--</option>
                            <option value="手机电脑">手机电脑</option>
                            <option value="其它">其它</option>
                            <option value="单车交通">单车交通</option>
                        </select>
                </li>
                <li>
                    <label>学校</label>
                    <select name="popSchool" id="two">
                        <option value="任意">--任意--</option>
                        <option value="成都理工大学">成都理工大学</option>
                        <option value="成都大学">成都大学</option>
                    </select>
                </li>
                <li>
                    <label >标题</label>
                    <select name="popTitle">

                    </select>
                </li>
                <li class="last">
                    <input type="button" class="tab-submit" value="提交"><input class="tab-cancel" type="button" value="取消">
                </li>
            </ul>
        </form>
        <div></div>
    </div>
</section>
<script src="../../javascripts/jquery.js"></script>
<script src="../../javascripts/jquery-dragMove.js"></script>
<script src="../javascripts/user.js"></script>
</body>
</html>